{
  "rootDir": "./src",
  "outDir": "./dist",
    "compilerOptions": {
      "module": "ESNext",
      "target": "ES2020",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "sourceMap": true,
      "resolveJsonModule": true
  },
  "include": ["src"]
}
